{
	"name": "@moodagent/common",
	"version": "16.12.0",
	"description": "Components common to the Moodagent web applications",
	"author": "Moodagent",
	"license": "MIT",
	"repository": "https://bitbucket.org/moodagent/web_ui_components/",
	"main": "dist/index.js",
	"scripts": {
		"build": "rollup -c",
		"dev": "rollup -c -w",
		"lint": "eslint . --max-warnings=0",
		"test": "jest --watch-all --coverage",
		"test:ci": "jest --ci --coverage",
		"storybook": "storybook dev -p 6006",
		"storybook:build": "storybook build -o storybook-static"
	},
	"peerDependencies": {
		"@mui/icons-material": "5.x",
		"@mui/lab": "^5.0.0-alpha.113",
		"@mui/material": "5.x",
		"i18next": "23.x",
		"react": "18.x",
		"react-dom": "18.x",
		"react-helmet": "6.x",
		"react-i18next": "14.x",
		"react-lazy-load-image-component": "1.x",
		"react-router-dom": "6.x",
		"swiper": "11.x"
	},
	"devDependencies": {
		"@babel/core": "^7.20.7",
		"@babel/eslint-parser": "^7.23.10",
		"@babel/plugin-proposal-class-properties": "^7.18.6",
		"@babel/plugin-proposal-export-default-from": "^7.18.10",
		"@babel/plugin-proposal-export-namespace-from": "^7.18.9",
		"@babel/plugin-transform-modules-commonjs": "^7.20.7",
		"@babel/plugin-transform-runtime": "^7.19.6",
		"@babel/preset-env": "^7.20.2",
		"@babel/preset-react": "^7.18.6",
		"@babel/preset-typescript": "^7.18.6",
		"@emotion/react": "^11.11.1",
		"@emotion/styled": "^11.10.5",
		"@mui/icons-material": "^5.11.0",
		"@mui/lab": "^5.0.0-alpha.113",
		"@mui/material": "^5.11.1",
		"@rollup/plugin-babel": "^6.0.4",
		"@rollup/plugin-commonjs": "^26.0.1",
		"@rollup/plugin-node-resolve": "^15.2.3",
		"@rollup/plugin-typescript": "^11.1.6",
		"@rollup/plugin-url": "^8.0.2",
		"@storybook/addon-essentials": "^8.1.1",
		"@storybook/addon-interactions": "^8.1.1",
		"@storybook/addon-links": "^8.1.1",
		"@storybook/addon-webpack5-compiler-babel": "^3.0.3",
		"@storybook/blocks": "^8.1.1",
		"@storybook/react": "^8.1.1",
		"@storybook/react-webpack5": "^8.1.1",
		"@storybook/test": "^8.1.1",
		"@stylistic/eslint-plugin": "^1.6.0",
		"@svgr/rollup": "^8.1.0",
		"@testing-library/jest-dom": "^6.1.4",
		"@testing-library/react": "^14.0.0",
		"@testing-library/user-event": "^14.5.1",
		"@types/jest": "^29.5.7",
		"@types/react-helmet": "^6.1.6",
		"@types/react-lazy-load-image-component": "^1.6.3",
		"@typescript-eslint/eslint-plugin": "^6.21.0",
		"@typescript-eslint/parser": "^6.21.0",
		"babel-loader": "^8.0.4",
		"babel-plugin-inline-react-svg": "^2.0.1",
		"babel-plugin-module-resolver": "^4.1.0",
		"eslint": "^8.56.0",
		"eslint-plugin-react": "^7.33.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"i18next": "^23.10.0",
		"jest": "^29.7.0",
		"jest-environment-jsdom": "^29.7.0",
		"postcss-styled-syntax": "^0.4.0",
		"postcss-syntax": "^0.36.2",
		"postcss-url": "^10.1.3",
		"prettier": "^2.8.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-helmet": "^6.1.0",
		"react-i18next": "^14.0.5",
		"react-lazy-load-image-component": "^1.6.0",
		"react-router-dom": "^6.6.0",
		"rollup": "^4.20.0",
		"rollup-plugin-cleaner": "^1.0.0",
		"rollup-plugin-copy": "^3.5.0",
		"rollup-plugin-peer-deps-external": "^2.2.4",
		"storybook": "^8.1.1",
		"storybook-addon-pseudo-states": "^3.1.1",
		"swiper": "^11.1.3",
		"tsconfig-paths-webpack-plugin": "^4.1.0",
		"typescript": "^4.9.4"
	},
	"files": [
		"dist"
	],
	"jest": {
		"collectCoverageFrom": [
			"src/components/**/*.{js,ts,tsx}",
			"src/errors/**/*.{js,ts,tsx}",
			"src/lib/**/*.{js,ts,tsx}",
			"!src/**/index.{js,ts,tsx}",
			"!src/**/*.stories.{js,ts,tsx}",
			"!src/errors/ERROR.ts",
			"!src/lib/CONSTANTS.js"
		],
		"moduleNameMapper": {
			"\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
			"(\\.css|/css)$": "<rootDir>/__mocks__/styleMock.js"
		},
		"modulePaths": [
			"<rootDir>/src/"
		],
		"setupFilesAfterEnv": [
			"<rootDir>/src/setupTests.tsx"
		],
		"testEnvironment": "jsdom",
		"transform": {
			"\\.([jt]sx?|mjs)$": "babel-jest"
		},
		"transformIgnorePatterns": [
			"node_modules/(?!(@babel|@moodagent/icons|@mui/material|swiper))"
		]
	}
}
